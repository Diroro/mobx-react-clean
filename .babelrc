{
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-typescript",
        { "onlyRemoveTypeImports": true }
      ]
    ],
    "plugins": [
      ["babel-plugin-typescript-decorators"],
      ["@babel/plugin-proposal-class-properties", { "loose" : true }],
      "babel-plugin-parameter-decorator"
    ]
  }